{
  "expo": {
    "name": "Earn Money",
    "slug": "earn-money-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.debarunlahiri.earnmoney",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to fetch your current address for registration."
      }
    },
    "android": {
      "package": "com.debarunlahiri.earnmoney",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "POST_NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#D4AF37"
      },
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "notification": {
      "color": "#D4AF37",
      "androidMode": "default",
      "androidCollapsedTitle": "Earn Money"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#D4AF37"
        }
      ]
    ],
    "web": {},
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "bd8fe442-a92d-4b4c-9397-82da381bd127"
      }
    }
  }
}